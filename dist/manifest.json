{
  "name": "Crypto Sumo Extension",
  "description": "Crypto Sumo Browser Extension",
  "version": "1.0.0",
  "manifest_version": 3,
  "icons": {},
  "action": {
    "default_popup": "public/popup.html",
    "default_title": "Crypto Sumo"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/content.ts-loader-DK31cXPj.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_idle",
      "css": [
        "assets/content-BUfS2wY2.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "assets/index-DMPdAPVQ.js",
        "assets/ccip-CZGRCy_-.js",
        "assets/content.ts-CddNh5Wc.js"
      ],
      "use_dynamic_url": false
    }
  ],
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ]
}
